---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    
    <title type="text" xml:lang="en">{{ site.root_desc }}</title>
    <link type="application/atom+xml" href="http://maxmanders.co.uk/feed/" rel="self"/>
    <link type="text" href="http://maxmanders.co.uk/" rel="alternate"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://maxmanders.co.uk</id>
    <author>
        <name>Max Manders</name>
    </author>
    <rights>Copyright (c), Max Manders; all rights reserved.</rights>
    
    {% for post in site.posts limit:20 %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://maxmanders.co.uk{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://maxmanders.co.uk{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}
</feed>
